{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base/legacy", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "no-useless-return": "off",
    "no-console": "warn",
    "no-plusplus": "off",
    "no-var": "error",
    "eol-last": "off",
    "no-alert": "warn",
    "indent": "off",
    "max-len": "off",
    "prefer-destructuring": "error",
    "no-underscore-dangle": "off",
    "no-useless-escape": "off",
    "no-new": "off",
    "no-undef": "off",
    "global-require": "off"
  }
}
